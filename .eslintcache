{\n    e.preventDefault();\n\n    const formData = new FormData(e.currentTarget);\n    const payload = Object.fromEntries(formData) as Record<TestId['input'], string>;\n\n    userApis[path](payload)\n      .then(res => {\n        const { data, status } = res;\n        if (status === authStatusCodeObj[path]) {\n          if (path === 'signin') {\n            localStorage.setItem('access_token', data.access_token);\n            navigate(pathsObj.todo);\n          }\n          if (path === 'signup') navigate(pathsObj.signin);\n        }\n      })\n      .catch(err => console.log(err));\n  };\n\n  return {\n    handleSubmit,\n    isBtnDisabled,\n    validationResult,\n    setValidationResult,\n  };\n};\n","/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/hooks/useTodo.ts",["103","104","105"],[],"import { HandleUpdate } from '../components/Todo';\nimport { todoApis, TodoItem, todoStatusObj } from '../apis/todo';\nimport { FormEventHandler, useState } from 'react';\n\nexport const useTodo = () => {\n  const [todos, setTodos] = useState<TodoItem[]>([]);\n\n  const handleSubmit: FormEventHandler<HTMLFormElement> = e => {\n    e.preventDefault();\n    const payload = new FormData(e.currentTarget).get('todo');\n\n    if (typeof payload === 'string') {\n      todoApis\n        .createTodo(payload)\n        .then(res => {\n          const { data, status } = res;\n          if (status === todoStatusObj.create) setTodos(prev => [...prev, data]);\n        })\n        .catch(err => console.log(err));\n    }\n  };\n\n  const handleUpdate: HandleUpdate = (e, data, idx) => {\n    const type = e.currentTarget.type;\n    const { id, todo, isCompleted } = data;\n    let payload;\n    if (type === 'checkbox') payload = { ...data, isCompleted: !isCompleted };\n    if (type === 'button') payload = { ...data, todo };\n\n    payload &&\n      todoApis\n        .updateTodo(id, payload)\n        .then(res => {\n          const { data, status } = res;\n          if (status === todoStatusObj.update) {\n            const newState = [...todos];\n            newState[idx] = data;\n            setTodos(newState);\n          }\n        })\n        .catch(err => console.log(err));\n  };\n\n  const handleDelete = (id: number, idx: number) => {\n    todoApis\n      .deleteTodo(id)\n      .then(res => {\n        const { status } = res;\n        if (status === todoStatusObj.delete) {\n          let newState = [...todos];\n          delete newState[idx];\n          setTodos(newState.filter(todo => todo !== undefined));\n        }\n      })\n      .catch(err => console.log(err));\n  };\n\n  return {\n    todos,\n    setTodos,\n    handleSubmit,\n    handleUpdate,\n    handleDelete,\n  };\n};\n","/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/index.tsx",[],[],"/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/pages/Home.tsx",["106"],[],"import { Outlet, useNavigate } from 'react-router-dom';\nimport styled from '@emotion/styled';\nimport { useEffect } from 'react';\nimport { pathsObj } from '../router/router';\n\nexport const Home = () => {\n  const navigate = useNavigate();\n\n  useEffect(() => {\n    const accessToken = localStorage.getItem('access_token');\n    if (accessToken) navigate(pathsObj.todo);\n  }, []);\n\n  return (\n    <StyledMain>\n      <Outlet />\n    </StyledMain>\n  );\n};\n\nconst StyledMain = styled.main`\n  display: grid;\n  place-items: center;\n  height: 100vh;\n`;\n","/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/pages/Signin.tsx",[],[],"/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/pages/Signup.tsx",[],[],"/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/pages/TodoList.tsx",["107","108"],[],"import { useEffect } from 'react';\nimport { useNavigate } from 'react-router-dom';\n\nimport { pathsObj } from '../router/router';\nimport { todoApis, todoStatusObj } from '../apis/todo';\nimport { Todo } from '../components/Todo';\nimport { useTodo } from '../hooks/useTodo';\n\nexport const TodoList = () => {\n  const navigate = useNavigate();\n  const { todos, setTodos, handleSubmit, handleUpdate, handleDelete } = useTodo();\n\n  useEffect(() => {\n    const accessToken = localStorage.getItem('access_token');\n    if (!accessToken) navigate(pathsObj.signin);\n    else {\n      todoApis\n        .getTodos()\n        .then(res => {\n          const { data, status } = res;\n          if (status === todoStatusObj.get) setTodos(data);\n        })\n        .catch(err => console.log(err));\n    }\n  }, []);\n\n  return (\n    <section\n      style={{\n        display: 'grid',\n        gap: '10px',\n      }}\n    >\n      <h1>Todo Form</h1>\n      <form onSubmit={handleSubmit}>\n        <input type=\"text\" data-testid=\"new-todo-input\" name=\"todo\" />\n        <button type=\"submit\" data-testid=\"new-todo-add-button\">\n          추가\n        </button>\n      </form>\n\n      <h1>Todo List</h1>\n      <ul>\n        {todos.map((todo, idx) => (\n          <Todo\n            key={todo.id}\n            todo={todo}\n            idx={idx}\n            handleUpdate={handleUpdate}\n            handleDelete={handleDelete}\n          />\n        ))}\n      </ul>\n    </section>\n  );\n};\n","/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/react-app-env.d.ts",[],[],"/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/reportWebVitals.ts",[],[],"/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/router/router.tsx",[],[],"/Users/su/Documents/dev/pre-onboarding-12th-1-14/src/setupTests.ts",[],[],{"ruleId":"109","severity":1,"message":"110","line":32,"column":6,"nodeType":"111","endLine":32,"endColumn":24,"suggestions":"112"},{"ruleId":"113","severity":1,"message":"114","line":59,"column":21,"nodeType":"115","messageId":"116","endLine":59,"endColumn":32},{"ruleId":"113","severity":1,"message":"114","line":19,"column":23,"nodeType":"115","messageId":"116","endLine":19,"endColumn":34},{"ruleId":"113","severity":1,"message":"114","line":41,"column":23,"nodeType":"115","messageId":"116","endLine":41